    "version": "0.2.0",
    "configurations": [
        {
            "name": "wonder_words (dev)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "fav-qs-app-token=YOUR_API_KEY"
            ]
        },
        {
            "name": "wonder_words (prod)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "prod",
                "--dart-define",
                "fav-qs-app-token=YOUR_API_KEY"
            ]
        },
        {
            "name": "wonder_words (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "fav_qs_api",
            "cwd": "packages/fav_qs_api",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "quote_repository",
            "cwd": "packages/quote_repository",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "user_repository",
            "cwd": "packages/user_repository",
            "request": "launch",
            "type": "dart"
        }
    ]
}